{
    "name": "nglasl/silverstripe-misdirection",
    "description": "This module allows both simple and regular expression link redirections based on customisable mappings, either hooking into a page not found or replacing the default automated URL handling.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "link mapping",
        "mapping",
        "url",
        "page not found",
        "404",
        "http 404",
        "redirection",
        "redirect"
    ],
    "homepage": "https://github.com/symbiote/silverstripe-misdirection",
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Nathan Glasl",
        "email": "nathan@symbiote.com.au"
    }],
    "require": {
        "silverstripe/framework": "^4 || ^5",
        "silverstripe/vendor-plugin": "^1 || ^2"
    },
    "require-dev": {
        "cambis/silverstripe-rector": "^0.5.1",
        "phpunit/phpunit": "^9.5",
        "syntro/silverstripe-phpstan": "^5",
        "nswdpc/ci-files": "^1"
    },
    "autoload": {
        "psr-4": {
            "nglasl\\misdirection\\": [
                "src/controllers/",
                "src/extensions/",
                "src/forms/",
                "src/objects/",
                "src/filters/",
                "src/services/"
            ],
            "nglasl\\misdirection\\tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    }
}
